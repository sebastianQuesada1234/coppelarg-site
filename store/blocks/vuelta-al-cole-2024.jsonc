        "blockClass": "body-container-home",
        "verticalAlign": "middle",
        "horizontalAlign": "CENTER"
        }
    },
    "flex-layout.row#vuelta-al-cole-2024-categorias": {
      "title": "Categorias",
      "children": [
        "rich-text#vuelta-al-cole-2024-categorias",
        "evento-category-slider#vuelta-al-cole-2024-categorias"
      ],
      "props": {
        "colSizing": "auto",
        "blockClass": "temp-evento-categorias"
      }
    },
    "rich-text#vuelta-al-cole-2024-categorias": {
      "title": "Titulo",
      "props": {
        "text": "Categorías destacadas",
        "blockClass": "temp-evento-estanteria-header"
      }
    },
    "evento-category-slider#vuelta-al-cole-2024-categorias": {
      "props": {
        "categories": [
          {
            "label": "Deportivas",
            "icon": "assets/vuelta-al-cole/deportivas.png",
            "link": "/vuelta-al-cole?map=category-1,productClusterIds&query=/deportes/551"
          },
          {
            "label": "Urbanas",
            "icon": "assets/vuelta-al-cole/zapas.png",
            "link": "/vuelta-al-cole?map=category-1,productClusterIds&query=/calzado/551"
          },
          {
            "label": "Tecnología",
            "icon": "assets/vuelta-al-cole/note.png",
            "link": "/vuelta-al-cole?map=category-1,productClusterIds&query=/notebooks-y-tecnologia/551"
          },
          {
            "label": "Celulares",
            "icon": "assets/vuelta-al-cole/celu.png",
            "link": "/vuelta-al-cole?map=category-1,productClusterIds&query=/celulares-telefonos-y-accesorios/551"
          },
          {
            "label": "Mochilas",
            "icon": "assets/vuelta-al-cole/mochilas.png",
            "link": "/vuelta-al-cole?map=category-3,productClusterIds&query=/mochilas/551"
          }
        ],
        "itemsPerPage": {
          "desktop": 6,
          "tablet": 4,
          "phone": 2
        }
      }
    },
    // Search
    "search-result-layout.customQuery#vuelta-al-cole-2024": {
      "props": {
        "blockClass": "coleccion",
        "querySchema": {
          "maxItemsPerPage": 20,
          "queryField": "551",
          "mapField": "productClusterIds",
          "skusFilter": "ALL_AVAILABLE",
          "simulationBehavior": "default"
        }
      },
      "blocks": [
        "search-result-layout.desktop#vuelta-al-cole-2024",
        "search-result-layout.mobile#vuelta-al-cole-2024",
        "coppelar.search-result:search-not-found-layout"
      ]
    },
    "search-result-layout.desktop#vuelta-al-cole-2024": {
      "children": [
        "flex-layout.row#result-vuelta-al-cole-2024"
      ],
      "props": {
        "preventRouteChange": true,
        "thresholdForFacetSearch": 10,
        "showProductsCount": true,
        "showFacetQuantity": true,
        "HiddenFacets": {
          "priceRange": true
        },
        "pagination": "show-more",
        "mobileLayout": {
          "mode1": "small",
          "mode2": "normal"
        }
      }
    },
    "search-result-layout.mobile#vuelta-al-cole-2024": {
        "children": [
          "flex-layout.row#searchinfomobile",
          "flex-layout.row#contentmobile",
          "flex-layout.row#pagination"
        ],
        "props": {
          "blockClass": "black-friday-collection",
          "pagination": "show-more",
          "mobileLayout": {
            "mode1": "small",
            "mode2": "normal"
          }
        }
    },
