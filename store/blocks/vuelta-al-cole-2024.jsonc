    },
    // Resultado desktop
    "flex-layout.row#result-vuelta-al-cole-2024": {
      "children": ["flex-layout.col#filter", "flex-layout.col#content"],
      "props": {
        "preventHorizontalStretch": true,
        "fullWidth": true,
        "blockClass" : "result-cyber-monday"
      }
