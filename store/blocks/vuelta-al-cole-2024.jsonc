        "props": {
        "blockClass": "body-container-home",
        "horizontalAlign": "CENTER",
        "fullWidth": true
        }
    },
    "flex-layout.col#vuelta-al-cole-2024-banner": {
        "children": [
        "list-context.image-list#vuelta-al-cole-2024"
