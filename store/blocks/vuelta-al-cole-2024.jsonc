        "blockClass": "body-container-home",
        "verticalAlign": "middle",
        "horizontalAlign": "CENTER"
        }
    },
    "list-context.image-list#vuelta-al-cole-2024": {
      "title": "Banner Principal",
      "children": [
        "slider-layout#temp-evento-images"
      ],
      "props": {
        "height": 1000,
        "fullWidth": true,
        "images": [
          {
            "image": "assets/vuelta-al-cole/desktop.png",
            "mobileImage": "assets/vuelta-al-cole/mobile.png"
          }
        ],
        "blockClass": "temp-evento-images"
      }
    },
    //Categorias
    "flex-layout.row#vuelta-al-cole-2024-body": {
        "children": [
        "flex-layout.col#vuelta-al-cole-2024-body"
        ],
        "props": {
        "blockClass": "body-container-home",
        "horizontalAlign": "CENTER",
        "fullWidth": true
        }
    },
    "flex-layout.col#vuelta-al-cole-2024-body": {
        "children": [
        "flex-layout.row#vuelta-al-cole-2024-categorias"
