{
  "name": "coppelarg-site",
  "vendor": "coppelar",
  "version": "0.1.0",
  "title": "Superior Checkout services",
  "description": "Superior Checkout services",
  "mustUpdateAt": "2023-06-02",
  "categories": [],
  "dependencies": {
    "vtex.store": "2.x",
    "coppelar.site-theme": "5.x",
    "vtex.flex-layout": "0.x"
  },
  "builders": {
    "react": "3.x",
    "store": "0.x"
  },
  "billingOptions": {
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "scripts": {},
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "coppelar.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "coppelar.myvtex.com",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "vtexid.vtexcommercestable.com.br",
        "path": "api/vtexid/apptoken/login"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "appssudamerica.coppel.com",
        "path": "*"
      }
    },
    {
      "name": "LogisticsAdmin"
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
