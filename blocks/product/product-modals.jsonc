{
  "modal-header#modal-header": {
    "props": {
      "showCloseButton": true,
      "blockClass": "modal-header"
    }
  },

  //TABLA-TALLES

  "modal-trigger#tabla-talles": {
    "children": ["rich-text#tabla-talles", "modal-layout#tabla-talles"],
    "props": {
      "blockClass": "tabla-talles"
    }
  },
  "rich-text#tabla-talles": {
    "props": {
      "text": "ver tabla de talles",
      "blockClass": "tabla-talles"
    }
  },
  "modal-layout#tabla-talles": {
    "children": ["modal-header#modal-header", "modal-content#tabla-talles"],
    "props": {
      "blockClass": "modal-talles"
    }
  },

  "modal-content#tabla-talles": {
    "children": []
  },
  "image#tabla-talles-image": {
    "props": {
      "src": "assets/images/tabla-talles.png",
      "blockClass": "tabla-talles"
    }
  },
  "size-chart#talles": {
    "props": {
      "groupName": "Detalles",
      "fieldName": "tabla de talles"
    }
  },

  //COSTO-ENVIO

  "modal-trigger#costo-envio": {
    "children": ["rich-text#costo-envio", "modal-layout#costo-envio"],
    "props": {
      "blockClass": "costo-envio"
    }
  },
  "image#costo-envio-icon": {
    "props": {
      "src": "assets/icons/costo-envio-icon.svg",
      "blockClass": "costo-envio-icon"
    }
  },
  "rich-text#costo-envio-label": {
    "props": {
      "text": "Envíos a todo el país *",
      "blockClass": "costo-envio-label"
    }
  },
  "rich-text#costo-envio": {
    "props": {
      "text": "Calcular costo de envío",
      "blockClass": "costo-envio"
    }
  },
  "rich-text#envios-caba-gba": {
    "props": {
      "text": "Envíos en 48hs Caba y GBA",
      "blockClass": "envios-caba-gba"
    }
  },
  "modal-layout#costo-envio": {
    "children": ["modal-header#modal-header", "modal-content#costo-envio"],
    "props": {
      "blockClass": "modal-container"
    }
  },
  "modal-content#costo-envio": {
    "children": [
      "rich-text#costo-envio-modal-title",
      "rich-text#costo-envio-modal-subtext"
    ]
  },
  "rich-text#costo-envio-modal-title": {
    "props": {
      "text": "![](assets/icons/costo-envio-icon.svg) Calcular costo de envío",
      "blockClass": "modal-title"
    }
  },
  "m3-shipping-simulator#product": {
    "props": {
      "filter": "soloEntrega"
    }
  },
  "rich-text#costo-envio-modal-subtext": {
    "props": {
      "text": "*Envíos a todo el país exceptuando las provincias de Tucumán y Misiones",
      "blockClass": ["modal-subtext", "small"]
    }
  },

  //SUCURSALES

  "modal-trigger#sucursales": {
    "children": ["rich-text#sucursales", "modal-layout#sucursales"],
    "props": {
      "blockClass": "sucursales"
    }
  },
  "image#sucursales-icon": {
    "props": {
      "src": "assets/icons/sucursales-icon.svg",
      "blockClass": "sucursales-icon"
    }
  },
  "rich-text#sucursales-label": {
    "props": {
      "text": "Retiro *gratis* en tienda",
      "blockClass": "sucursales-label"
    }
  },
  "rich-text#sucursales": {
    "props": {
      "text": "![](assets/icons/icon-store-blue.svg) Retiro gratis en tienda",
      "blockClass": "sucursales"
    }
  },
  "modal-layout#sucursales": {
    "children": ["modal-header#modal-header", "modal-content#sucursales"],
    "props": {
      "blockClass": ["modal-container", "large"]
    }
  },
  "modal-content#sucursales": {
    "children": ["rich-text#sucursales-modal-title", "m3-puntos-de-recogida"]
  },
  "rich-text#sucursales-modal-title": {
    "props": {
      "text": "Tiendas disponibles\n*Encuentra tu tienda más cercana*",
      "blockClass": ["modal-title", "header-title", "no-margin"]
    }
  },

  //CUOTAS

  "modal-trigger#cuotas": {
