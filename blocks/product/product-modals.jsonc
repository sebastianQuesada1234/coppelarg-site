  "rich-text#cuotas-label": {
    "props": {
      "text": "Conocé las cuotas con tu tarjeta *",
      "blockClass": "cuotas-label"
    }
  },
  "rich-text#cuotas": {
    "props": {
      "text": "Ver métodos de pago",
      "blockClass": "cuotas"
    }
  },
  "modal-layout#cuotas": {
    "children": ["modal-header#modal-header", "modal-content#cuotas"],
    "props": {
      "blockClass": ["modal-container", "installments"]
    }
  },
  "modal-content#cuotas": {
    "children": [
      "rich-text#cuotas-modal-title",
      "rich-text#cuotas-modal-subtitle",
      "installments-list#p2"
