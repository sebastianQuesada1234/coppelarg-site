    "props": {
      "text": "Selecciona tu método de pago",
      "blockClass": ["modal-title", "modal-cuotas"]
    }
  },
  "rich-text#cuotas-modal-subtitle": {
    "props": {
      "text": "*Descubre a cuantas cuotas puedes pagar tu artículo*",
      "blockClass": ["modal-title", "modal-cuotas"]
    }
  },
