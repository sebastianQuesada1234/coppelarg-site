{
  "flex-layout.row#flixmedia-wrapper": {
    "title": "Descripcion Flixmedia",

    "props": {
      "marginTop": 4,
      "blockClass": "flixmedia-wrapper"
    },
    "children": ["disclosure-layout#flixmedia"]
  },
  "disclosure-layout#flixmedia": {
    "children": ["disclosure-trigger#flixmedia", "disclosure-content#flixmedia"]
  },
  "disclosure-trigger#flixmedia": {
    "children": ["rich-text#trigger-flixmedia"],
    "props": {
      "blockClass": "disclosure-flixmedia"
    }
  },
  "disclosure-content#flixmedia": {
    "children": ["product-description.flixmedia"]
  },
  "rich-text#trigger-flixmedia": {
    "title": "Texto del botton",

    "props": {
      "text": "Descripcion del fabricante",
      "blockClass": "flixmedia-title"
    }
  }
}
