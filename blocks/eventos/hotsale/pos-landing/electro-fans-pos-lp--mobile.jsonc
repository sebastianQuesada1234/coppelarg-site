        "props": {
          "blockClass": "ef2023-banners"
        }
      },
      "image#hs-pos-lp-main-banner-mobile": {
        "props": {
          "blockClass": "ef2023-banner",
          "src": "https://coppelar.vteximg.com.br/arquivos/pos-hs2023-main-banner--mobile.jpg"
        }
      },
      "image#hs-pos-lp-descuento-banner-mobile": {
        "props": {
          "blockClass": "ef2023-banner",
          "src": "https://coppelar.vteximg.com.br/arquivos/pos-hs2023-descuento-banner--mobile.jpg"
        }
      }
}