  "rich-text#CO_02_dc-title": {
    "props": {
      "blockClass": "pf-topic",
      "text": "¿Cuáles son los medios de envío?"
    }
  },
  "flex-layout.col#CO_02_dc-conteudo": {
    "children": [
      "rich-text#CO_02_dc_01"
    ]
  },
  "rich-text#CO_02_dc_01": {
    "props": {
      "blockClass": "pf-text",
      "text": "**Coppel Express:** C.A.B.A. y GBA\n**Zippin:** Provincia de Buenos Aires e interior del país, exceptuando Misiones y Tucumán."
    }
  },

  //DC 03
  "disclosure-layout#CO_03_dc": {
    "children": ["disclosure-trigger#CO_03_dc", "disclosure-content#CO_03_dc"]
  },
  "disclosure-trigger#CO_03_dc": {
